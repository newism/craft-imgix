{
    "name": "newism/craft-imgix",
    "description": "Imgix integration for CraftCMS",
    "license": "proprietary",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "imgix"
    ],
    "authors": [
        {
            "name": "Leevi Graham",
            "email": "leevi@newism.com.au"
        }
    ],
    "support": {
        "email": "support@newism.com.au",
        "source": "https://github.com/newism/craft-imgix",
        "docs": "https://github.com/newism/craft-imgix/blob/master/README.md"
    },
    "require": {
        "craftcms/cms": "^4.0|^5.6.0",
        "imgix/imgix-php": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Newism\\Imgix\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "handle": "newism-imgix",
        "name": "Imgix Asset Transformer",
        "developer": "Newism",
        "developerUrl": "https://newism.com.au"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    }
}
